<?xml version="1.0" encoding="utf-8"?>
<!--This defines the seekbar.
Much of this code can be found here:
https://stackoverflow.com/questions/32212259/android-how-to-change-default-seekbar-thickness-->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <size
        android:height="@dimen/seek_bar_thumb_size"
        android:width="@dimen/seek_bar_thumb_size"
        />
    <solid android:color="@color/seek_bar_progress"/>
</shape>